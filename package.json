{
    "name": "nexosis-dashboard",
    "version": "1.0.0",
    "description": "Dashboard used to interface and manage the Nexosis Machine Learning API",
    "author": "Nexosis",
    "license": "UNLICENSED",
    "private": true,
    "scripts": {
        "test": "node node_modules/elm-test/bin/elm-test",
        "test-watch": "node node_modules/elm-test/bin/elm-test --watch",
        "test-output": "node node_modules/elm-test/bin/elm-test --report=junit > junit.xml",
        "remove-dist": "rimraf ./dist && rimraf ./temp",
        "clean-dist": "npm run remove-dist && mkdir dist && mkdir temp",
        "elm-install": "elm-install",
        "elm-install-tests": "cd tests/ && elm-install --skip-update",
        "prebuild": "npm run clean-dist",
        "build": "set TEMP=.\\temp\\&&babel-node tools/build.js",
        "open-app": "babel-node tools/srcServer.js",
        "open-dist": "babel-node tools/distServer.js",
        "analyse": "node node_modules/elm-analyse/js/bin/index.js -s",
        "analyse-bundle": "babel-node ./tools/analyseBundle.js"
    },
    "dependencies": {
        "balloon-css": "^0.5.0",
        "loggly-jslogger": "^2.2.2",
        "nexosis-styles": "^1.0.15",
        "prismjs": "^1.13.0",
        "stacktrace-js": "^2.0.0"
    },
    "devDependencies": {
        "autoprefixer": "^8.1.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.4",
        "babel-preset-env": "^1.6.1",
        "babel-preset-stage-1": "^6.24.1",
        "browser-sync": "^2.23.6",
        "chalk": "^2.3.0",
        "compression": "^1.7.2",
        "css-loader": "^0.28.11",
        "elm": "0.18.0",
        "elm-analyse": "0.13.3",
        "elm-format": "^0.7.0-exp",
        "elm-github-install": "^1.6.1",
        "elm-hot-loader": "^0.5.4",
        "elm-live": "2.7.5",
        "elm-test": "0.18.12",
        "elm-webpack-loader": "^4.5.0",
        "exports-loader": "^0.7.0",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.0.7",
        "mini-css-extract-plugin": "^0.4.0",
        "node-sass": "^4.7.2",
        "optimize-css-assets-webpack-plugin": "^4.0.0",
        "postcss-loader": "^2.1.0",
        "rimraf": "^2.6.2",
        "sass-loader": "^6.0.7",
        "style-loader": "^0.20.3",
        "uglifyjs-webpack-plugin": "^1.2.4",
        "webpack": "^4.1.1",
        "webpack-bundle-analyzer": "^2.11.0",
        "webpack-combine-loaders": "^2.0.3",
        "webpack-dev-middleware": "^3.0.1",
        "webpack-hot-middleware": "^2.21.2",
        "webpack-md5-hash": "^0.0.6"
    }
}
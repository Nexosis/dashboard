{
    "name": "nexosis-dashboard",
    "version": "1.0.0",
    "description": "Dashboard used to interface and manage the Nexosis Machine Learning API",
    "author": "Nexosis",
    "license": "UNLICENSED",
    "private": true,
    "scripts": {
        "test": "node node_modules/elm-test/bin/elm-test",
        "test-watch": "node node_modules/elm-test/bin/elm-test --watch",
        "test-output": "node node_modules/elm-test/bin/elm-test --report=junit > junit.xml",
        "remove-dist": "rimraf ./dist",
        "clean-dist": "npm run remove-dist && mkdir dist",
        "prebuild": "npm run clean-dist",
        "build": "babel-node tools/build.js",
        "open-app": "babel-node tools/srcServer.js",
        "open-dist": "babel-node tools/distServer.js",
        "analyse": "node node_modules/elm-analyse/js/bin/index.js -s",
        "analyse-bundle": "babel-node ./tools/analyseBundle.js"
    },
    "dependencies": {
        "balloon-css": "^0.5.0",
        "loggly-jslogger": "^2.2.2",
        "prismjs": "^1.10.0",
        "stacktrace-js": "^2.0.0"
    },
    "devDependencies": {
        "autoprefixer": "^7.2.5",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.1",
        "babel-preset-stage-1": "^6.24.1",
        "browser-sync": "^2.23.6",
        "chalk": "^2.3.0",
        "css-loader": "^0.28.9",
        "elm": "0.18.0",
        "elm-analyse": "0.13.3",
        "elm-format": "^0.7.0-exp",
        "elm-hot-loader": "^0.5.4",
        "elm-live": "2.7.5",
        "elm-test": "0.18.12",
        "elm-webpack-loader": "^4.4.0",
        "exports-loader": "^0.7.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.6",
        "html-webpack-plugin": "^2.30.1",
        "node-sass": "^4.7.2",
        "postcss-loader": "^2.1.0",
        "rimraf": "^2.6.2",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.20.1",
        "uglify-js": "3.3.8",
        "uglifyjs-webpack-plugin": "^1.1.8",
        "webpack": "^3.10.0",
        "webpack-bundle-analyzer": "^2.10.0",
        "webpack-combine-loaders": "^2.0.3",
        "webpack-dev-middleware": "^2.0.4",
        "webpack-hot-middleware": "^2.21.0",
        "webpack-md5-hash": "^0.0.6"
    }
}
